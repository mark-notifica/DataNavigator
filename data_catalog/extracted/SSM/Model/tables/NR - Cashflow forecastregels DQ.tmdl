table 'NR - Cashflow forecastregels DQ'

	column Regelkey_Pivot
		dataType: int64
		isNullable: false
		formatString: 0
		sourceProviderType: int
		summarizeBy: sum
		sourceColumn: Regelkey_Pivot

		annotation SummarizationSetBy = Automatic

	column ForecastcashflowKey
		dataType: int64
		isNullable: false
		formatString: 0
		sourceProviderType: int
		summarizeBy: none
		sourceColumn: ForecastcashflowKey

		annotation SummarizationSetBy = Automatic

	column Datum
		dataType: dateTime
		formatString: General Date
		sourceProviderType: date
		summarizeBy: none
		sourceColumn: Datum

		annotation SummarizationSetBy = Automatic

	column waarde
		dataType: double
		sourceProviderType: decimal(18, 2)
		summarizeBy: sum
		sourceColumn: waarde

		annotation SummarizationSetBy = Automatic

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	column CashflowCategorieKey
		dataType: int64
		formatString: 0
		sourceProviderType: int
		summarizeBy: none
		sourceColumn: CashflowCategorieKey

		annotation SummarizationSetBy = Automatic

	column CashflowCategorie
		dataType: string
		sourceProviderType: nvarchar(250)
		summarizeBy: none
		sourceColumn: CashflowCategorie

		annotation SummarizationSetBy = Automatic

	partition 'NR - Cashflow forecastregels DQ' = m
		mode: directQuery
		queryGroup: NotificaRAAS
		source = ```
				let
				    Source = Sql.Database(#"7_serverAppdata", #"8_databaseAppdata", [Query="
				SELECT  [Regelkey_Pivot]
				      ,[ForecastcashflowKey]
				      ,Dateadd(day,-1,[Datum]) as Datum
				      ,[waarde]
				      ,[CashflowCategorieKey]
				      ,[CashflowCategorie]
				  FROM [RAAS].[Forecastcashflowregels] a
				  left join [RAAS].[Forecastcashflow] b on a.[ForecastcashflowKey] = b.regelkey
				  where b.klantnummer ="&Text.From(#"1_klantnummer")&"
				  
				    ", CreateNavigationProperties=false])
				in
				    Source
				```

	annotation PBI_ResultType = Table

