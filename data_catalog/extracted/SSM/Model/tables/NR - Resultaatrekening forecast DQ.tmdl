/// Voorheen: NR - Forecast DQ
table 'NR - Resultaatrekening forecast DQ'

	column Regelkey
		dataType: int64
		isNullable: false
		formatString: 0
		sourceProviderType: int
		summarizeBy: none
		sourceColumn: Regelkey

		annotation SummarizationSetBy = Automatic

	column Aanmaakdatum
		dataType: dateTime
		formatString: dd-mm-yy
		sourceProviderType: datetime
		summarizeBy: none
		sourceColumn: Aanmaakdatum

		annotation SummarizationSetBy = Automatic

		annotation PBI_FormatHint = {"isDateTimeCustom":true}

	column Aanmaker
		dataType: string
		sourceProviderType: nvarchar(250)
		summarizeBy: none
		sourceColumn: Aanmaker

		annotation SummarizationSetBy = Automatic

	column Wijzigdatum
		dataType: dateTime
		formatString: General Date
		sourceProviderType: datetime
		summarizeBy: none
		sourceColumn: Wijzigdatum

		annotation SummarizationSetBy = Automatic

	column Wijziger
		dataType: string
		sourceProviderType: nvarchar(250)
		summarizeBy: none
		sourceColumn: Wijziger

		annotation SummarizationSetBy = Automatic

	column Versie
		dataType: string
		sourceProviderType: nvarchar(250)
		summarizeBy: none
		sourceColumn: Versie

		annotation SummarizationSetBy = Automatic

	column Klantnummer
		dataType: int64
		isNullable: false
		formatString: 0
		sourceProviderType: int
		summarizeBy: none
		sourceColumn: Klantnummer

		annotation SummarizationSetBy = Automatic

	column AdministratieKey
		dataType: int64
		isNullable: false
		formatString: 0
		sourceProviderType: int
		summarizeBy: none
		sourceColumn: AdministratieKey

		annotation SummarizationSetBy = Automatic

	column BedrijfseenheidKey
		dataType: int64
		formatString: 0
		sourceProviderType: int
		summarizeBy: none
		sourceColumn: BedrijfseenheidKey

		annotation SummarizationSetBy = Automatic

	column Administratie
		dataType: string
		sourceProviderType: nvarchar(250)
		summarizeBy: none
		sourceColumn: Administratie

		annotation SummarizationSetBy = Automatic

	column Bedrijfseenheid
		dataType: string
		sourceProviderType: nvarchar(250)
		summarizeBy: none
		sourceColumn: Bedrijfseenheid

		annotation SummarizationSetBy = Automatic

	column Jaar
		dataType: int64
		formatString: 0
		sourceProviderType: int
		summarizeBy: none
		sourceColumn: Jaar

		annotation SummarizationSetBy = Automatic

	partition 'NR - Resultaatrekening forecast DQ' = m
		mode: directQuery
		queryGroup: NotificaRAAS
		source = ```
				let
				    Source = Sql.Database(#"7_serverAppdata", #"8_databaseAppdata", [Query="
				SELECT  [Regelkey]
				      ,[Aanmaakdatum]
				      ,[Aanmaker]
				      ,[Wijzigdatum]
				      ,[Wijziger]
				      ,[Versie]
				      ,[Klantnummer]
				      ,[AdministratieKey]
				      ,case when BedrijfseenheidKey is null 
						then cast([AdministratieKey]as varchar)+'999' 
						else [BedrijfseenheidKey]
					  end as [BedrijfseenheidKey]
				      ,[Administratie]
				      ,[Bedrijfseenheid]
				      ,[Jaar]
				  FROM [RAAS].[Forecastresultaat]
				  where klantnummer ="&Text.From(#"1_klantnummer")&"
				    
				    ", CreateNavigationProperties=false])
				in
				    Source
				```

	annotation PBI_ResultType = Table

