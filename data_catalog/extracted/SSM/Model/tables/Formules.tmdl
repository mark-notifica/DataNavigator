	measure Projectfase = ```
			VAR __projectniveau =
			 CALCULATE(
			    MIN( Projecten[Niveau] )
			    , ALLEXCEPT( Projecten,Projecten[Hoofdproject],Projecten[Niveau 2 Project],Projecten[Niveau 3 Project] )
			)
			
			VAR __inscope= 
			     ISINSCOPE(Projecten[Hoofdproject])
			     
			VAR result =
			    IF(
			    __inscope
			    , SWITCH(
			        __projectniveau
			        , 1, MAX( Projecten[Hoofdprojectfase] )
			        , 2, MAX( Projecten[Niveau 2 projectfase] )
			        , 3, MAX( Projecten[Niveau 3 projectfase] )
			        , BLANK()
			    )
			)
			
			RETURN 
			    result
			```
		displayFolder: 1. Semantisch model\1.03 Projecten\Projectselectie

		changedProperty = IsHidden

	measure Projectstatus = ```
			VAR __projectniveau =
			 CALCULATE(
			    MIN( Projecten[Niveau] )
			    , ALLEXCEPT( Projecten,Projecten[Hoofdproject],Projecten[Niveau 2 Project],Projecten[Niveau 3 Project] )
			)
			
			VAR __inscope= 
			     ISINSCOPE(Projecten[Hoofdproject])
			
			VAR result =
			IF(
			    __inscope
			    , SWITCH(
			        __projectniveau
			        , 1, MAX( Projecten[Hoofdprojectstatus] )
			        , 2, MAX( Projecten[Niveau 2 projectstatus] )
			        , 3, MAX( Projecten[Niveau 3 projectstatus] )
			        , BLANK()
			    )
			)
			
			RETURN
			    result
			```
