	measure '% standmaand t.o.v. standmaand LY' = DIVIDE([Delta standmaand - standmaand LY],[CoA mutatie in de standmaand o.b.v. standdatum])
		formatString: 0%;-0%;0%
		displayFolder: 2. Rapport Specifieke Objecten\2.01 Best Practice Financieel\Forecast resultaatrekening\CoA rekeningschema\Ratio

	measure 'CoA actual LY in standmaand o.b.v. standdatum' = ```
			
			VAR _classification         = SELECTEDVALUE('NR - CoA schema'[Classificatie type])
			VAR _is_subtotaal           = _classification = "Subtotaal"
			VAR _is_geen_omzet          = _classification <> "Omzet" 
			VAR _correctSubtotalLevel   = _is_subtotaal                && ISINSCOPE('NR - CoA schema'[Niveau1]) && NOT(ISINSCOPE('NR - CoA schema'[Niveau2]))
			VAR _is_legeregel           = _classification = "Leeg"    && ISINSCOPE('NR - CoA schema'[Niveau1]) && NOT(ISINSCOPE('NR - CoA schema'[Niveau2]))
			VAR _einddatum_gesecteerde_standmaand = DATE(YEAR(MAX('Kalender standdatums'[Einddatum Maand]))-1,MONTH(MAX('Kalender standdatums'[Einddatum Maand])),DAY(MAX('Kalender standdatums'[Einddatum Maand])))
			VAR _begindatum_gesecteerde_standmaand = DATE(YEAR(MIN('Kalender standdatums'[Begindatum Maand]))-1,MONTH(MIN('Kalender standdatums'[Begindatum Maand])),DAY(MIN('Kalender standdatums'[Begindatum Maand])))
			
			
			VAR _eerste_kalenderdatum =
			CALCULATE(
			    MIN(Kalender[Datum])
			    ,ALL('Kalender standdatums')
			    , FILTER(
			        ALL( Kalender[Datum] )
			        , Kalender[Datum] >= _begindatum_gesecteerde_standmaand
			        && Kalender[Datum] <= _einddatum_gesecteerde_standmaand
			    )
			)
			
			VAR _credit_eliminatie_Resultaatrekening = 
			CALCULATE(
			    SUM( 'Eliminatie journaalregels'[Credit] )
			    , 'Eliminatie journaalregels'[Rubriektype] = "W"
			    ,NOT ( ISBLANK('NR - CoA mapping DQ'[CoA_code] ) )
			    ,FILTER(
			        ALL( Kalender )
			        , Kalender[Datum] >= _begindatum_gesecteerde_standmaand
			        && Kalender[Datum] <= _einddatum_gesecteerde_standmaand
			    )
			) 
			
			
			VAR _credit_exploitatie     = 
			CALCULATE(
			    SUM( 'Journaalregels'[Creditbedrag] )
			    ,'Rubrieken'[Type] = "W"
			    ,NOT ( ISBLANK('NR - CoA mapping DQ'[CoA_code] ) )
			    ,ALL('Kalender standdatums')
			    ,FILTER(
			        ALL( Kalender )
			        , Kalender[Datum] >= _begindatum_gesecteerde_standmaand
			        && Kalender[Datum] <= _einddatum_gesecteerde_standmaand
			    )
			) - _credit_eliminatie_Resultaatrekening
			
			VAR _debet_eliminatie_Resultaatrekening = 
			CALCULATE(
			    SUM( 'Eliminatie journaalregels'[Debet] )
			    , 'Eliminatie journaalregels'[Rubriektype] = "W"
			    ,NOT ( ISBLANK('NR - CoA mapping DQ'[CoA_code] ) )
			    ,FILTER(
			        ALL( Kalender )
			        , Kalender[Datum] >= _begindatum_gesecteerde_standmaand
			        && Kalender[Datum] <= _einddatum_gesecteerde_standmaand
			    )
			)
			 
			VAR _debet_exploitatie      = 
			CALCULATE(
			    SUM( 'Journaalregels'[Debetbedrag] )
			    , 'Rubrieken'[Type] = "W"
			    ,NOT ( ISBLANK('NR - CoA mapping DQ'[CoA_code]  ) )
			    ,ALL('Kalender standdatums')
			    ,FILTER(
			        ALL( Kalender )
			        , Kalender[Datum] >= _begindatum_gesecteerde_standmaand
			        && Kalender[Datum] <= _einddatum_gesecteerde_standmaand
			    )
			)  - _debet_eliminatie_Resultaatrekening
			
			
			VAR _mutatie_eploitatie     = _credit_exploitatie - _debet_exploitatie -- Reguliere mutaties op rubriek--
			
			VAR _subtotaalWaarde =
			    CALCULATE(
			        [Mutatie resultaat met mapping in standmaand LY o.b.v. standdatum]
			        , FILTER(
			            ALL( 'NR - CoA schema' )
			            , 'NR - CoA schema'[Niveau2_sortering] <= MAX( 'NR - CoA schema'[Niveau2_sortering] )
			        )
			    )
			
			VAR result =
			    SWITCH(
			        TRUE
			        ,_is_legeregel,0
			        ,NOT(_is_subtotaal) && _is_geen_omzet,-_mutatie_eploitatie
			        ,NOT(_is_subtotaal),_mutatie_eploitatie
			        ,_correctSubtotalLevel,_subtotaalWaarde
			    )/1000
			    
			    
			RETURN
			    result
