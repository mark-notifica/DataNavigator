			    ,'Project kosten en geboekte uren'[Kostenbron] = "Inkoop"
			    --,'Kosten'[Kostenstatus] = "Definitief"
			    ,'Project kosten en geboekte uren'[internationale code] = "570410"
			)
			
			VAR result = verrekenprijs_met_inleen - verrekenprijs_inleners
			
			
			RETURN
