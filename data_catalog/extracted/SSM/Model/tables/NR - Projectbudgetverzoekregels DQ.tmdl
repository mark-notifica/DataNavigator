	partition 'NR - Projectbudgetverzoekregels DQ' = m
		mode: directQuery
		queryGroup: NotificaRAAS
		source = ```
				let
				    Source = Sql.Database(#"7_serverAppdata", #"8_databaseAppdata", [Query="
				SELECT 
					[BudgetverzoekRegelKey]
					,[BudgetverzoekKey]
					,[ProjectKey]
					,[BestekparagraafKey]
					,[KostensoortKey]
					,[TaakKey]
					,[KostenplaatsKey]
					,[Bestekparagraaf]
					,[Categorie]
					,[Invoerbedrag]
					,[AantalUur]
					,[Tarief]
					,[Dekkingspercentage]
					,[Kostprijs]
					,[Klantnummer]
					,[Klantnaam]
					,[Aangepast uurtarief per verzoek] as [Aangepast uurtarief]
				FROM [RAAS].[Budgetverzoekregels] 
				where klantnummer = "&Text.From(#"1_klantnummer")&"
				
				union all
				
				SELECT  
					NULL as [BudgetverzoekRegelKey]
					,a.[BudgetverzoekKey]
					,a.ProjectKey
					,a.BestekParagraafKey as [BestekparagraafKey]
					,NULL as [KostensoortKey]
					,NULL as [TaakKey]
					,NULL as [KostenplaatsKey]
					,NULL as [Bestekparagraaf]
					,NULL as [Categorie]
					,NULL as [Invoerbedrag]
					,NULL as [AantalUur]
					,NULL as [Tarief]
					,NULL as [Dekkingspercentage]
					,NULL as [Kostprijs]
					,a.[Klantnummer]
					,NULL as [Klantnaam]
					,NULL as [Aangepast uurtarief]
				FROM [dbo].[NT_Budgetverzoeken] a
				where a.[Budget_handhaven_JN] = 'J'
				and a.Klantnummer = "&Text.From(#"1_klantnummer")&"
				    ", CreateNavigationProperties=false])
				in
				    Source
				```

