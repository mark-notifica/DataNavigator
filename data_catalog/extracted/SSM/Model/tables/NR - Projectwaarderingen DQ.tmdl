	partition 'NR - Projectwaarderingen DQ' = m
		mode: directQuery
		queryGroup: NotificaRAAS
		source =
				let
				    Source = Sql.Database(#"7_serverAppdata",#"8_databaseAppdata", [Query="
				SELECT [ProjectwaarderingKey]
				      ,[Aanmaakdatum]
				      ,[Wijzigdatum]
				      ,[Aanmaker]
				      ,[Wijziger]
				      ,[Boekdatum]
				      ,[Opmerking]
					  ,Status
				      ,[Titel]
					  ,cast([ProjectwaarderingKey] as varchar)
						+' - '
						+Titel
						+' ('
						+ Status
						+')'
						as Projectwaardering
				    ,klantnummer
				  FROM [RAAS].[Projectwaarderingen]
				  where klantnummer ="&Text.From(#"1_klantnummer")&"
				    "])
				in
				    Source

	annotation PBI_ResultType = Table

	annotation PBI_NavigationStepName = Navigation

