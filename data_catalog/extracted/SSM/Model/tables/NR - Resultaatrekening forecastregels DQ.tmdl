/// Voorheen: NR - Forecastregels DQ
table 'NR - Resultaatrekening forecastregels DQ'

	column Regelkey_Pivot
		dataType: int64
		isHidden
		isNullable: false
		formatString: 0
		sourceProviderType: int
		summarizeBy: sum
		sourceColumn: Regelkey_Pivot

		annotation SummarizationSetBy = Automatic

	column ForecastResultaatKey
		dataType: int64
		isNullable: false
		formatString: 0
		sourceProviderType: int
		summarizeBy: none
		sourceColumn: ForecastResultaatKey

		annotation SummarizationSetBy = Automatic

	column Datum
		dataType: dateTime
		formatString: General Date
		sourceProviderType: date
		summarizeBy: none
		sourceColumn: Datum

		annotation SummarizationSetBy = Automatic

	column waarde
		dataType: double
		sourceProviderType: decimal(18, 2)
		summarizeBy: sum
		sourceColumn: waarde

		annotation SummarizationSetBy = Automatic

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	column 'CoA Code'
		dataType: string
		sourceProviderType: nvarchar(250)
		summarizeBy: none
		sourceColumn: CoA Code

		annotation SummarizationSetBy = Automatic

	column 'Niveau 1'
		dataType: string
		sourceProviderType: nvarchar(250)
		summarizeBy: none
		sourceColumn: Niveau 1

		annotation SummarizationSetBy = Automatic

	column 'Niveau 2'
		dataType: string
		sourceProviderType: nvarchar(250)
		summarizeBy: none
		sourceColumn: Niveau 2

		annotation SummarizationSetBy = Automatic

	partition 'NR - Resultaatrekening forecastregels DQ' = m
		mode: directQuery
		queryGroup: NotificaRAAS
		source = ```
				let
				    Source = Sql.Database(#"7_serverAppdata", #"8_databaseAppdata", [Query="
				  SELECT [Regelkey_Pivot]
				      ,[ForecastResultaatKey]
				      ,[Datum]
				      ,[waarde]
				      ,[CoA Code]
				      ,[Niveau 1]
				      ,[Niveau 2]
				  FROM [RAAS].[Forecastresultaatregels] a
				  left join [RAAS].[Forecastresultaat] b on a.ForecastResultaatKey = b.regelkey
				  where b.klantnummer ="&Text.From(#"1_klantnummer")&"
				  
				    ", CreateNavigationProperties=false])
				in
				    Source
				```

	annotation PBI_ResultType = Table

