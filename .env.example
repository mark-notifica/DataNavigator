# =====================================================
# DATANAVIGATOR V1 - ENVIRONMENT CONFIGURATION
# =====================================================

# === SOURCE DATABASE ===
# The database you want to CATALOG (extract metadata from)
SOURCE_DB_HOST=localhost
SOURCE_DB_PORT=5432
SOURCE_DB_NAME=your_source_database
SOURCE_DB_USER=your_user
SOURCE_DB_PASSWORD=your_password

# === CATALOG DATABASE ===
# Where DataNavigator stores its own data (descriptions, etc.)
CATALOG_DB_HOST=localhost
CATALOG_DB_PORT=5432
CATALOG_DB_NAME=datanavigator
CATALOG_DB_USER=your_user
CATALOG_DB_PASSWORD=your_password